#Exercise 5 
Ldatstu_matching_sss_jss_first <- subset(Ldatstu_matching_sss_jss,school==1)
Ldatstu_matching_sss_jss_first_high <- Ldatstu_matching_sss_jss_first[order(Ldatstu_matching_sss_jss_first$score,decreasing = TRUE), ] 
sample <- Ldatstu_matching_sss_jss_first_high[1:20000,]
sample$choice_rev <- as.factor(sample$choice_rev)
sample$choice_rev <- as.numeric(sample$choice_rev)
Nchoice_sample <- n_distinct(sample$choice_rev)

set.seed(1232)
x <- sample(1:nrow(sample),100)
small_sample <- sample[x,]
small_sample$choice_rev <- as.factor(small_sample$choice_rev)
small_sample$choice_rev <- as.numeric(small_sample$choice_rev)

#Likelihood Function
mlogit_like_fun = function(param,data)
{
  score      =  data$score
  ch         =  data$choice_rev
  ni = nrow(data)
  nj = length(unique(data[,22]))
  nj_par = nj-1
  ut = mat.or.vec(ni,nj)
  # multinomial logit
  pn1 = param[1:nj_par]
  pn2 = param[(nj_par+1):(2*nj_par)]
  ut[,1]<-rep(0,ni)
  for (j in 2:nj)
  {
    ut[,j] = pn1[j-1] + score*pn2[j-1]
  }
  prob  = exp(ut)       
  prob  = sweep(prob,MARGIN=1,FUN="/",STATS=rowSums(prob))
  # match prob to actual choices
  probc = NULL
  for (i in 1:ni)
  {
    probc[i] = prob[i,ch[i]]
  }
  probc[probc>0.999999] = 0.999999
  probc[probc<0.000001] = 0.000001
  like = sum(log(probc))
  return(-like)
}

m <- n_distinct(small_sample$choice_rev)

#Package Result
multinom_model <- multinom(choice_rev~score,data=small_sample,maxit=10000)
sum <- summary(multinom_model)
start_ideal <- c(sum$coefficients[,1],sum$coefficients[,2])
marginal_effects(multinom_model)

#Choose Starting Value
set.seed(123)
noise   = rnorm(78,0,0.05)
start   = c(start_ideal,noise)
res1    = optim(start,fn=mlogit_like_fun,method="BFGS",control=list(trace=6,REPORT=1,maxit=10000),data=small_sample,hessian=TRUE)
par_m   = res1$par

#Marginal Effect
  #People-Choice Matrix
prob_matrix <- mat.or.vec(100,m)
prob_matrix[,1] <- rep(0,100)
par_constant<-par_m[1:39]
par_score <- par_m[40:78]
for (j in 2:m){
  prob_matrix[,j] = par_constant[j-1] + small_sample$score*par_score[j-1]
}
prob_matrix <- exp(prob_matrix)
prob_matrix <- sweep(prob_matrix,MARGIN=1,FUN="/",STATS=rowSums(prob_matrix))
colnames(prob_matrix) <- c(1:m)
  #Match Pij
small_sample$probc<-0
for (i in 1:100){
  small_sample$probc[i] = prob_matrix[i,small_sample$choice_rev[i]]
}
  #Match coefficients
choice_rev  <- c(1:m)
c_constant  <- c(0,par_m[1:m-1])
c_score     <- c(0,par_m[m:78])
aa <- data.frame(cbind(choice_rev,c_constant))
bb <- data.frame(cbind(choice_rev,c_score))
small_sample <- merge(small_sample,aa,by='choice_rev')
small_sample <- merge(small_sample,bb,by='choice_rev')
  #Computation by formula
small_sample$beta_i_bar <- sum(small_sample$c_score * small_sample$probc)
small_sample$me <- small_sample$probc*(small_sample$c_score - small_sample$beta_i_bar)
package_me <- marginal_effects(multinom_model)

#Exercise 6 
small_sample_clogit <- merge(small_sample,quality_rev,by='choice_rev') 
clogit_like_fun = function(param,data){
  quality     =  data$meanscore
  ch          =  data$choice_rev
  ni = nrow(data)
  nj = length(unique(data[,1]))
  ut = mat.or.vec(ni,nj)
  # conditional logit
  pn1 = c(0,param[1:nj-1])
  for (j in 1:nj)
  {
    ut[,j] = pn1[j] + param[nj]*quality 
  }
  prob  = exp(ut)       
  prob  = sweep(prob,MARGIN=1,FUN="/",STATS=rowSums(prob))
  # match prob to actual choices
  probc = NULL
  for (i in 1:ni)
  {
    probc[i] = prob[i,ch[i]]
  }
  probc[probc>0.999999] = 0.999999
  probc[probc<0.000001] = 0.000001
  like = sum(log(probc))
  return(-like)
}

#Package Results
clogit_model <- clogit(choice_rev~meanscore+strata(V1.x),data=small_sample_clogit)

#Optimization
start_2 = runif(m,0,1)
res2    = optim(start_2,fn=clogit_like_fun,method="BFGS",control=list(trace=6,REPORT=1,maxit=10000),data=small_sample_clogit,hessian=TRUE)
res2$par

co_constant <- c(0,res2$par[1:39])
co_quality  <- res2$par[40]

# Marginal Effects for Conditional Logit
prob_matrix_c <- mat.or.vec(100,m)
for (j in 1:m){
  prob_matrix_c[,j] = co_constant[j] +  co_quality * small_sample_clogit$meanscore
}
prob_matrix_c <- exp(prob_matrix_c)
prob_matrix_c <- sweep(prob_matrix_c,MARGIN=1,FUN="/",STATS=rowSums(prob_matrix_c))
 
small_sample_clogit$probc<-0
for (i in 1:100){
  small_sample_clogit$probc[i] = prob_matrix_c[i,small_sample_clogit$choice_rev[i]]
}

probc1 <- unique(small_sample_clogit$probc)
beta <- res2$par[40]
ME <- mat.or.vec(m,m)

for (i in 1:40) {
  for (j in 1:40) {
    if (i==j){
      ME[i,j]<-probc1[i]*(1-probc1[i])*beta
    }
    else{
      ME[i,j]<-probc1[i]*(-1)*probc1[j]*beta}
  }
}
